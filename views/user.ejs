<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Bepity - Profile</title>
    <link rel="stylesheet" href="/stylesheets/style.css">
</head>

<body id="profile">

    <!-- <img src="https://cdnb.artstation.com/p/assets/images/images/010/905/753/large/chengwei-pan-1.jpg?1526875963" alt="" id="profile_backround"> -->

    <aside id="flexwrap">
        <div id="textwrap">
            <span id="text1"><a href="#">Home</a></span>
            <span id="text2">Profile</span>
            <span id="text4">Contact</span>
            <span id="text5">Settings</span>
        </div>
        <div id="iconwrap">
            <menu id="menu1"><i class="fas fa-home"></i></menu>
            <menu id="menu2"><i class="fas fa-user-circle"></i></menu>
            <menu id="menu4"><i class="fas fa-envelope"></i></menu>
            <menu id="menu5"><i class="fas fa-cog"></i></menu>
        </div>
    </aside>

    <div class="profile_wrapper">
        <div id="profile_pic"><img src="<%= icon_link%>" alt=""></div>

        <div id="uppdate">
            <button>Update</button>
            <p>latest 57h</p>

        </div>

        <div id="recent">
            <div class="recent_champs">
                <img src="img/match_history.jpg" alt="">
                <h3>Lee Sin</h3>
                <p>100% of <br> games played</p>
            </div>

            <div class="recent_champs">
                <img src="img/match_history.jpg" alt="">
                <h3>Lee Sin</h3>
                <p>100% of <br> games played</p>
            </div>
            
            <div class="recent_champs">
                <img src="img/match_history.jpg" alt="">
                <h3>Lee Sin</h3>
                <p>100% of <br> games played</p>
            </div>
            
            <div class="recent_champs">
                <img src="img/match_history.jpg" alt="">
                <h3>Lee Sin</h3>
                <p>100% of <br> games played</p>
            </div>
            
            <div class="recent_champs">
                <img src="img/match_history.jpg" alt="">
                <h3>Lee Sin</h3>
                <p>100% of <br> games played</p>
            </div>
            
            <div class="recent_champs">
                <img src="img/match_history.jpg" alt="">
                <h3>Lee Sin</h3>
                <p>100% of <br> games played</p>
            </div>
        </div>

        <div id="name">
            <h2><%= name%>
                <p>level - <%= level%> </p>
            </h2>
        </div>

        <div id="rank">
            <img src="img/rannk2.png" alt="">
            <h3><%= tier %>  <%= rank%>
                <p>solo/duo</p>
            </h3>
        </div>

        <div id="match_history">
            <% for(var i = 0; i < 8; i++) { %>
                <div id="match1" class="match">
                    <div class="match_info">
                        <h4><%= matchdata[i].info.gameMode %></h4>

                        <% if (matchdata[i].info.participants[participantID[i]].win == true){ %>
                            <p>Victory</p>
                          <% } else{ %>  
                            <p>Defeat</p>
                         <% } %>
                    </div>
                    <img src="img/match_history.jpg" alt="">
                    <div class="summoner_spells">
                        <img src="img/Flash.png" alt="">
                        <img src="img/smite.png" alt="">
                    </div>

                    <div class="match_champ">
                        <h2><%= matchdata[i].info.participants[participantID[i]].championName %></h2>
                        <p>KP - 100%</p>
                    </div>

                    <div class="match_stats">
                        <h4>Kda</h4>
                        <p>4 / 2 / 0</p>
                    </div>

                </div>
            <% } %>

        </div>

    </div>

    <script src="https://kit.fontawesome.com/ad2feffdfb.js" crossorigin="anonymous"></script>
</body>

</html>